{
    "name": "cannonphp/app",
    "description": "Cannon MVC is an exceptional web application framework that embraces an expressive and elegant syntax. ",
    "config": {
        "sort-packages":true
    },
    "require": {
        "twig/twig": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Core\\": "Core/",
            "Engine\\": "Engine/", 
            "App\\": "App/"
        }
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10"
    },
    "scripts": {
        "phpstan": "vendor/bin/phpstan analyze -c phpstan.neon"
    },
    "license": "MIT"
}
